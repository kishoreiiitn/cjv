    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.4",
    "commitizen": "^4.1.2",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.2.0",
    "framer-motion": "^1.11.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lerna": "^3.22.1",
    "lodash": "^4.17.15",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
