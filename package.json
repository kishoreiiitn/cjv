{
	"name": "flowjv-workspace",
	"private": true,
	"scripts": {
		"commit": "npm t && git-cz",
		"dev": "lerna exec -- npm run dev",
		"testw": "jest --watch",
		"test": "lerna exec -- npm t && jest",
		"publish-docs": "sh ./publish-docs.sh",
		"prepublish": "npm run test",
		"publish": "lerna publish --force-publish"
	},
	"repository": {
		"url": "https://www.github.com/pkishoez/flowjv"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^3.1.3",
		"@material-ui/core": "^4.11.0",
		"@types/jest": "^26.0.9",
		"@types/lodash": "^4.14.159",
		"@types/node": "^14.0.27",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"autoprefixer": "^10.1.0",
		"commitizen": "^4.1.2",
		"cssnano": "^4.1.10",
		"cz-conventional-changelog": "^3.2.0",
		"framer-motion": "^3.1.1",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"lerna": "^3.22.1",
		"lodash": "^4.17.19",
		"postcss": "^8.2.1",
		"postcss-cli": "^8.3.1",
		"postcss-import": "^14.0.0",
		"postcss-nested": "^5.0.3",
		"precss": "^4.0.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"sass": "^1.26.10",
		"tailwindcss": "^2.0.2",
		"ts-jest": "^26.1.4",
		"typescript": "^4.1.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test"
		}
	},
	"dependencies": {
		"flowjv": "file:packages/flowjv",
		"flowjv-builder": "file:packages/flowjv-builder",
		"flowjv-react": "file:packages/flowjv-react",
		"flowjv-react-custom": "file:packages/flowjv-react-custom"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
